<launch>
  <node pkg="vicon"
    type="vicon"
    name="vicon"
    output="screen">
    <param name="vicon_server" value="alkaline2"/>
    <param name="calib_files_dir" value="$(find vicon)/calib"/>
  </node>
  <group ns="vicon">
    <include file="$(find vicon_odom)/launch/vicon_odom.launch">
      <arg name="model" value="$(arg model)"/>
    </include>
  </group>
</launch>
